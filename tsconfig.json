{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "sourceMap": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "baseUrl": "src",
    "paths": {
      "@models/*": ["lib/models/*"],
      "@modules/*": ["lib/modules/*"],
      "@lib/*": ["lib/*"]
    },
    "esModuleInterop": true
  }
}
